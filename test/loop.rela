
sum = 0
for i in 10
	sum = sum+i
end
assert(sum == 45)

sum = 0
vec = [1,2,3]
for v in vec
	sum = sum+v
end
assert(sum == 6)

sum = 0
vec = [1,2,3]
for k,v in vec
	sum = sum+k
end
assert(sum == 3)

sum = 0
keys = ""
map = { a = 1, b = 2, c = 42 }
for k,v in map
	sum = sum+v
	keys = "$keys$k"
end
assert(sum == 45)
assert(keys == "abc")
